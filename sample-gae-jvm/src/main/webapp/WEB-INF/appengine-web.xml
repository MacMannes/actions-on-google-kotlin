<?xml version="1.0" encoding="utf-8"?>
<!-- USED FOR THE PROD VERSION OF APP ENGINE when updated with ./update.sh-->
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>kotlin-actions-sdk</application>
    <version>v2</version> <!-- READ FROM ~/.gradle/gradle.properties MAESTRO_APP_ENGINE_VERSION-->
    <runtime>java8</runtime>
    <warmup-requests-enabled>true</warmup-requests-enabled>

    <automatic-scaling></automatic-scaling>

    <threadsafe>true</threadsafe>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/logging.properties"/>
    </system-properties>
    <resource-files>
        <include path="/**.json"/>
    </resource-files>
</appengine-web-app>
